pipeline {
    agent {
        label 'linux'
    }

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/umerBlueiris/nodejsbanking.git'
            }
        }

        stage('Build') {
            steps {
                sh 'npm install'
                sh 'npm run build'
            }
        }

   
    }
}

