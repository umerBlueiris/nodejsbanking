pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/umerBlueiris/nodejsbanking.git'
            }
        }

        stage('Build') {
            steps {
                sh 'npm install'
                sh 'npm run build'
            }
        }
    } // Closing brace for stages block
} // Closing brace for pipeline block

